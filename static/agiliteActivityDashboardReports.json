{
  "extensions": [
    {
      "path": "communities",
      "application": "Activity Dashboard Reports",
      "payload": {
        "include-files": [
          "agiliteCore.js",
          "chart.js",
          "xml2json.js",
          "agiliteActivityDashboardReports.js"
        ],
        "include-repo": {
          "name": "customization-repo-1"
        }
      },
      "name": "Activity Dashboard Reports Extension",
      "type": "com.ibm.customizer.ui"
    }
  ],
  "name": "Activity Dashboard Reports",
  "description": "Uses Chart.js to create graphical charts in the Community's Rich Content widget, that reports on Active and Completed To Do items",
  "services": [
    "Customizer"
  ],
  "title": "Activity Dashboard Reports"  
}